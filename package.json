{
    "name": "gitbook-plugin-devops",
    "author": "FankWeiJie",
    "version": "1.0.3",
    "description": "gitbook plugin module",
    "engines": {
        "gitbook": "*"
    },
    "main": "index.js",
    "keywords": [
        "gitbook",
        "plugin",
        "analytics",
        "comment"
    ],
    "license": "Apache-2.0",
    "homepage": "https://gitee.com/fank243/gitbook-plugin-devops#readme",
    "repository": {
        "type": "git",
        "url": "https://gitee.com/fank243/gitbook-plugin-devops.git"
    },
    "bugs": {
        "url": "https://gitee.com/fank243/gitbook-plugin-devops/issues"
    },
    "gitbook": {
        "properties": {
            "bd_token": {
                "required": false,
                "type": "string",
                "default": "",
                "title": "Baidu Analytics Token"
            },
            "google_search": {
                "required": false,
                "type": "string",
                "default": "",
                "title": "Google Search meta"
            },
            "google_analytics": {
                "required": false,
                "type": "string",
                "default": "",
                "title": "Google Analytics UA-ID"
            },
            "livere_token": {
                "required": false,
                "type": "string",
                "default": "",
                "title": "Livere Comment Token"
            }
        }
    },
    "dependencies": {}
}
